<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1b2330;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lavalamp.kpclient.contollers.AddCurrencyController">
   <children>
      <TextField fx:id="moneyValueTextField" alignment="CENTER" layoutX="233.0" layoutY="175.0" onKeyTyped="#TextFieldChanged" promptText="Введите сумму" />
      <Button fx:id="addMoneyButton" layoutX="249.0" layoutY="253.0" mnemonicParsing="false" onAction="#AddMoneyButtonClick" prefHeight="25.0" prefWidth="118.0" style="-fx-background-radius: 20;" text="Пополнить" />
      <ToolBar prefHeight="40.0" prefWidth="615.0">
        <items>
          <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#LogoutButtonClick" prefHeight="15.0" prefWidth="27.0">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/arrow-back-icon.png" />
                     </image></ImageView>
               </graphic></Button>
        </items>
      </ToolBar>
   </children>
</AnchorPane>
